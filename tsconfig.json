{
    "extends": "@apify/tsconfig",
    "compilerOptions": {
        "module": "ES2022",
        "target": "ES2022",
        "outDir": "dist",
        "noUnusedLocals": false,
        "lib": ["DOM"],
        "sourceMap": true,
        "esModuleInterop": true,
        "moduleResolution": "nodenext",
        "skipLibCheck": true
    },
    "include": [
        "./src/**/*"
    ]
}
